{
  "name": "crystal-components",
  "version": "1.0.0-rc.1",
  "description": "A collection of UI components built on top of Metal.js (JavaScript) and Bootstrap 3 (HTML/CSS)",
  "moduleType": [
    "es6"
  ],
  "keywords": [
    "crystal",
    "metal",
    "metaljs"
  ],
  "ignore": [
    "*",
    "!build/**/*"
  ],
  "dependencies": {
    "crystal-affix": "metal/crystal-affix#master",
    "crystal-dropdown": "metal/crystal-dropdown#master",
    "crystal-modal": "metal/crystal-modal#master",
    "crystal-popover": "metal/crystal-popover#master",
    "crystal-progressbar": "metal/crystal-progressbar#master",
    "crystal-scrollspy": "metal/crystal-scrollspy#master",
    "crystal-slider": "metal/crystal-slider#master",
    "crystal-tooltip": "metal/crystal-tooltip#master",
    "crystal-treeview": "metal/crystal-treeview#master"
  }
}
