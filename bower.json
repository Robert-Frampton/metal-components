{
  "name": "metal-components",
  "version": "1.0.0-rc.4",
  "description": "A collection of UI components built on top of Metal.js (JavaScript) and Bootstrap 3 (HTML/CSS)",
  "moduleType": [
    "es6"
  ],
  "keywords": [
    "metal",
    "metaljs"
  ],
  "ignore": [
    "*",
    "!build/**/*"
  ],
  "dependencies": {
    "metal-affix": "metal/metal-affix#master",
    "metal-dropdown": "metal/metal-dropdown#master",
    "metal-modal": "metal/metal-modal#master",
    "metal-popover": "metal/metal-popover#master",
    "metal-progressbar": "metal/metal-progressbar#master",
    "metal-scrollspy": "metal/metal-scrollspy#master",
    "metal-slider": "metal/metal-slider#master",
    "metal-tooltip": "metal/metal-tooltip#master",
    "metal-treeview": "metal/metal-treeview#master"
  }
}
